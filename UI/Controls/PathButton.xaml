<Button x:Class="DotLogix.UI.Controls.PathButton"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        x:Name="Self" Background="Transparent" BorderBrush="{x:Null}" BorderThickness="0" mc:Ignorable="d">
    <Path Data="{Binding ElementName=Self, Path=Data}"
          Stretch="{Binding ElementName=Self, Path=Stretch}"
          Fill="{Binding ElementName=Self, Path=Fill}"
          Stroke="{Binding ElementName=Self, Path=Stroke}"
          StrokeThickness="{Binding ElementName=Self, Path=StrokeThickness}" />
</Button>